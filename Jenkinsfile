          steps { 
              echo 'building a new application ....'
          }      
      }
      stage("test") {
          steps { 
               echo 'testing a new application ....'
          }      
      }
      stage("deploy") {
          steps { 
               echo 'deploying a new application ....'
          }      
      }
  }
}
